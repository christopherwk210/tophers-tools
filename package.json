{
  "name": "tophers-tools",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p readme type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build --base=/tools/",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "readme": "ts-node --project tools/tsconfig-tools.json tools/generate-readme-tool-list.ts",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@jaames/iro": "^5.5.2",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "chart.js": "^4.3.0",
    "json5": "^2.2.3",
    "lottie-web": "^5.12.2",
    "nprogress": "^0.2.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "weq8": "0.2.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^7.0.2",
    "sass": "^1.63.6",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  }
}
